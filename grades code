class Main {
    public static void studentresults(int marks){
        if(marks>=90){
            System.out.println("garde=A");
        }else if (marks>=60){
            System.out.println("grade=B");
        }else{
            System.out.print("grade=C");
        }
        
    }
    public static void main(String[] args) {
        String name="je";
        int regno=192372180;
       int marks=90;
     

       studentresults(marks);
       
        
    }
}
